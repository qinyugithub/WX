<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="bag.wxsshtest.entities.Bar" table="BAR" dynamic-update="true" dynamic-insert="true" >

		<id name="id" type="java.lang.Integer">
			<column name="ID" />
			<generator class="native" />
		</id>

		<property name="pic" type="java.lang.String">
			<column name="PIC" length="70" />
		</property>

		<property name="eventype" type="java.lang.String">
			<column name="EVENTTYPE"  length="50"/>
		</property>

		<property name="address" type="java.lang.String">
			<column name="ADDRESS" length="100" />
		</property>

		<property name="cnumber" type="java.lang.String">
			<column name="CNUMBER" length="30" />
		</property>

		<property name="bartime" type="java.util.Date">
			<column name="BARTIME" />
		</property>

		<property name="bartime2" type="java.util.Date">
			<column name="BARTIME2" />
		</property>

		<property name="contact" type="java.lang.String">
			<column name="CONTACT" length="20" />
		</property>
		
		<property name="introduce" type="text">
			<column name="INTRODUCE" />
		</property>

		<property name="title" type="text">
			<column name="TITLE" />
		</property>

		<property name="username" type="java.lang.String">
			<column name="USERNAME"  length="100" />
		</property>

		<property name="city" type="java.lang.String">
			<column name="CITY" length="50" />
		</property>

		<property name="undertaker" type="java.lang.String">
			<column name="UNDERTAKER" length="100" />
		</property>

		<property name="longitude" type="java.lang.String">
			<column name="LONGITUDE" length="50" />
		</property>

		<property name="latitude" type="java.lang.String">
			<column name="LATITUDE" length="50" />
		</property>

		<property name="event_state" type="java.lang.String">
			<column name="EVENT_STATE" length="10" default="0" />
		</property>

		<property name="eventkey" type="java.lang.Boolean">
			<column name="EVENTKEY" default="1" />
		</property>


	</class>
</hibernate-mapping>
